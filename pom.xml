  <version>0.0.1-SNAPSHOT</version>
  <name>DynamoDB-Tutorial</name>
  <description>This is a Dynamo DB Tutorial</description>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
